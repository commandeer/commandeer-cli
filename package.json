{
  "name": "commandeer-cli",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Commandeer CLI - Command Line Interface",
  "homepage": "https://getcommandeer.com",
  "author": {
    "name": "Commandeer, Inc.",
    "email": "contact@getcommandeer.com",
    "url": "https://getcommandeer.com"
  },
  "main": "./lib/index.js",
  "bin": {
    "commandeer": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "yarn run build && yarn run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && commandeer",
    "refresh": "rm -rf ./node_modules ./yarn-lock.json && yarn install",
    "test": "sudo npm i -g && commandeer"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "clear": "0.1.0",
    "commander": "7.2.0",
    "figlet": "1.5.0",
    "path": "0.12.7"
  },
  "devDependencies": {
    "@types/node": "15.12.1",
    "depcheck": "1.4.1",
    "lerna": "4.0.0",
    "nodemon": "2.0.7",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "keywords": [
    "cli",
    "commandeer",
    "aws",
    "localstack",
    "docker"
  ]
}
